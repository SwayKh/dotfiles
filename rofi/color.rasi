/* foreground, blue, darkblue, are set to BBCCDD*/
* {
  foreground: #D99887; //#F2C395;
  oldforeground: #87A2AA;
  background: #333333;
  highlight: #F5F5F5;
  old-highlight: #F1594F;
  red: #973D46;
  darkRed: #973D46;
  blue: #BBCCDD;
  yellow: #E0AE4A;
  darkYellow: #E0AE4A;
  border: #1B1E25;
  darkBlue: #04142D;

  altBlue: #ACBAF9;
  altDarkBlue: #1F1E2E;
  altWhite: #F1D8D4;

  lightGray: #777777;
  mediumGray: #454545;
  darkGray: #232323;
  black: #111111;
  darkBlack: #000000;
}

window {
  border-color: @lightGray;
}

listview {
  background-color: @black;
}

textbox {
  text-color: @lightGray;
  background-color: @black;
}

entry {
  enabled: true;
  padding: 0px 5px;
  text-color: @lightGray;
  background-color: @black;
}

prompt {
  padding: 0px 5px;
  text-color: @lightGray;
}

textbox-prompt-colon {
  expand: false;
  margin: 0px;
  str: "ï’‰ ";
  text-color: @lightGray;
}

inputbar {
  background-color: @black;
  spacing: 8px;
  padding: 8px;
  margin: 0px;
  border: 0px;
  border-radius: 0px;
  border-color: @black;
  children: ["textbox-prompt-colon", entry];
}

mode-switcher {
  text-color: @lightGray;
  background-color: @black;
}

element {
  orientation: horizontal;
  border: 0;
  padding: 3px;
  text-color: @lightGray;
  background-color: @black;
}

element-icon {
  size: 22;
}

element normal.normal {
  background-color: @black;
  text-color: @lightGray;
}

element normal.urgent {
  background-color: @red;
  text-color: @lightGray;
}

element normal.active {
  background-color: @black;
  text-color: @lightGray;
}

element selected.normal {
  text-color: @black;
  background-color: @lightGray;
}

element selected.urgent {
  background-color: @red;
  text-color: @black;
}

element selected.active {
  background-color: @yellow;
  text-color: @black;
}

element alternate.normal {
  background-color: @black;
  text-color: @lightGray;
}

element alternate.urgent {
  background-color: @black;
  text-color: @lightGray;
}

element alternate.active {
  background-color: @black;
  text-color: @lightGray;
}

element-text {
  cursor: inherit;
  highlight: #F1EDEE;
  text-color: inherit;
}
